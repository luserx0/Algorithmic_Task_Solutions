#include <cstdio>
#include <stack>
#include <vector>
#include <string>
#include <algorithm>
#include <iostream>

using namespace std;

#define MAXN 1000000



int i, j, a, numbers[MAXN];
int n;


void Read(){
    for (i=0; i<n; i++){
        scanf("%d", &a);
        numbers[i]=a;
    }
}


long MaxSum(){
    //Kadane's Algorithm
    int max_so_far = 0, max_ending_here = 0;

    for(i = 0; i < n; i++){
        max_ending_here = max_ending_here + numbers[i];
        
        return max_so_far;

}


int main(){
    //freopen("boat.in","r",stdin);
    //freopen("boat.out","w",stdout);

    scanf("%d", &n);

    Read();//check

    long result = MaxSum();//douleyei alla vgazei periergous xaraktires kai dn 3erw ti einai.

    return 0;
}
